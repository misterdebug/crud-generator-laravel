{
    "name": "mrdebug/crudgen",
    "description": "Create a Laravel Crud in a few seconds",
    "license": "MIT",
    "keywords": [
        "laravel",
        "crud",
        "rest",
        "generator",
        "rest-api",
        "laravel-package",
        "crud-generator",
        "laravel-crud-generator",
        "composer-package",
        "laravel-crud",
        "laravel8",
        "laravel9",
        "laravel10"
    ],
    "authors": [
        {
            "name": "Jonathan Escarem",
            "email": "jonathan@labo-dev.fr"
        }
    ],
    "require":
    	{
    		"php": ">=8.0.0",
        "spatie/laravel-html": "^3.4"
    	},
    "autoload":
    {
        "psr-4":
        {
            "Mrdebug\\Crudgen\\": "src/"
        }
    },
    "extra":
    {
        "laravel":
        {
            "providers":
            [
                "Mrdebug\\Crudgen\\CrudgenServiceProvider"
            ]
        }
    }
}
